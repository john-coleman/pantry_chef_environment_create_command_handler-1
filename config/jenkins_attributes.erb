{
  "authorization" => {
    "sudo" => {
      "groups" => [ "sysadmin" ],
      "users" => [ "deploy", "ubuntu" ],
      "passwordless" => true,
      "include_sudoers_d" => true
    }
  },
  "build_agent" => {
    "git_ssh_path" => "C:\\Jenkins",
    "java_jdk_physical_path" => "C:/Program Files/Java/jdk1.7.0_40/bin",
    "jenkins_working_directory" => "C:\\Jenkins"
  },
  "build_essential" => {
    "compiletime" => true
  },
  "java" => {
    "windows" => {
      "url" => "https://s3-eu-west-1.amazonaws.com/mirror/java/jdk-7u40-windows-x64.exe"
    }
  },
  "jenkins" => {
    "http_proxy" => {
      "host_name" =>"<%= @message['team_name'] %>.<%= @message['domain'] %>",
      "variant" => "nginx"
    },
    "node" => {
      "auth_ad_domain" => "EXAMPLE",
      "auth_enabled" => true,
      "auth_user" => "ProvisionerUsername",
      "auth_password" => "ProvisionerPassword",
      "home" => "C:\\Jenkins",
      "service_user" => "EXAMPLE\\ProvisionerUsername",
      "service_user_password" => "ProvisionerPassword"
    },
    "server" => {
      "host" => "<%= @message['team_name'] %>.<%= @message['domain'] %>",
      "plugins" => [
        "active-directory",
        "ansicolor",
        "git",
        "git-client",
        "github",
        "greenballs",
        "htmlpublisher",
        "ldap",
        "mailer",
        "msbuild",
        "parameterized-trigger",
        "radiatorviewplugin",
        "scm-sync-configuration",
        "translation",
        "versionnumber"
      ],
      "port" => 8080,
      "url" => "http://<%= @message['team_name'] %>.<%= @message['domain'] %>:8080"
    },
  },
  "nginx" => {
    "default_site_enabled" => false
  },
  "openssh" => {
    "client" => {
      "strict_host_key_checking" => "no"
    }
  },
  "wonga" => {
    "jenkins" => {
      "git_user_data_bag"=> "users",
      "git_user_data_bag_item"=> "ProvisionerUsername",
      "git_ssh_hosts"=> [
        {
          "host"=> "github.com",
          "StrictHostKeyChecking"=> "no",
          "User"=> "git"
        },
        {
          "host"=> "git.example.com",
          "StrictHostKeyChecking"=> "no",
          "User"=> "git2"
        }
      ],
      "scm_sync_configuration_data_bag"=> "<%= @message["team_name"] %>",
      "scm_sync_configuration_data_bag_item"=> "jenkins_scm_sync_configuration_plugin"
    }
  }
}
